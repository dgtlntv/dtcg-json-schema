{
    "description": "Resolver with inline modifier",
    "version": "2025.10",
    "resolutionOrder": [
        {
            "type": "modifier",
            "name": "theme",
            "description": "Color theme",
            "contexts": {
                "light": [
                    {
                        "color": {
                            "background": {
                                "$type": "color",
                                "$value": {
                                    "colorSpace": "srgb",
                                    "components": [1, 1, 1]
                                }
                            }
                        }
                    }
                ],
                "dark": [
                    {
                        "color": {
                            "background": {
                                "$type": "color",
                                "$value": {
                                    "colorSpace": "srgb",
                                    "components": [0.1, 0.1, 0.1]
                                }
                            }
                        }
                    }
                ]
            },
            "default": "light"
        }
    ]
}
